{
    "compilerOptions": {
      "types": ["lua-types/5.2", "@kumaya1109/nekoland-server-types"],
      "target": "esnext",
      "lib": ["esnext"],
      "typeRoots": ["node_modules"],
      "rootDir": "src/server",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true
    },
    "tstl": {
      "luaTarget": "5.2",
      "luaLibImport": "require"
    },
    "include": ["src/server"]
  }